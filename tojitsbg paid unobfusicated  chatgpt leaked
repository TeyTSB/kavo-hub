            for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
                animTrack:Stop()
            end
            
            local AnimAnim = Instance.new("Animation")
            AnimAnim.AnimationId = "rbxassetid://14611879113"
            local Anim = Humanoid:LoadAnimation(AnimAnim)
            local startTime = 0
            Anim:Play()
            Anim:AdjustSpeed(2)
            Anim.TimePosition = startTime
            Anim:AdjustSpeed(1.6)
            message = "Even with ur jujutsu sorcery, you'll be unable to defeat me."  game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
