
local runGear = Instance.new("Tool")
runGear.Name = "Toji Run"
runGear.RequiresHandle = false
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")
local originalWalkSpeed = humanoid.WalkSpeed
local firstAnimationId = "rbxassetid://18897115785"
local secondAnimationId = "rbxassetid://18897118507"
local runSpeedMultiplier = 4  -- Speed multiplier set to 4 times
local UIS = game:GetService("UserInputService")
local function preventMovement()
    UIS.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Keyboard then
            local key = input.KeyCode
            if key == Enum.KeyCode.W or key == Enum.KeyCode.A or key == Enum.KeyCode.S or key == Enum.KeyCode.D then
                input:Capture()
            end
        end
    end)
end
local function onActivated()
    -- Prevent manual movement
    preventMovement()
    -- Increase speed
    humanoid.WalkSpeed = originalWalkSpeed * runSpeedMultiplier
    -- Load and play the first animation
    local firstAnimation = Instance.new("Animation")
    firstAnimation.AnimationId = firstAnimationId
    local animator = humanoid:FindFirstChildOfClass("Animator") or humanoid:WaitForChild("Animator")
    local firstAnimationTrack = animator:LoadAnimation(firstAnimation)
    firstAnimationTrack:Play()
    -- Start moving the player forward
    local runLoop
    runLoop = game:GetService("RunService").Heartbeat:Connect(function()
        -- Continuously set the walk speed to prevent any changes
        humanoid.WalkSpeed = originalWalkSpeed * runSpeedMultiplier
        rootPart.CFrame = rootPart.CFrame * CFrame.new(0, 0, -humanoid.WalkSpeed / 60)
    end)
    -- Wait 5 seconds, then stop running and play the second animation
    wait(5)
    runLoop:Disconnect()
    humanoid.WalkSpeed = originalWalkSpeed
    firstAnimationTrack:Stop()
    -- Play the second animation
    local secondAnimation = Instance.new("Animation")
    secondAnimation.AnimationId = secondAnimationId
    local secondAnimationTrack = animator:LoadAnimation(secondAnimation)
    secondAnimationTrack:Play()
    -- Stop the second animation after its third second
    wait(3)
    secondAnimationTrack:Stop()
end
runGear.Activated:Connect(onActivated)
runGear.Parent = player.Backpack
local ToolNameHere = "Toji Flex."
local StunLength = 6 -- Stun duration in seconds
local AutoActivate = false
local AnimationId = "17108683768"
local AnimationEnabled = true
local SoundId = ""
local AutoUnEquip = true

-- Additional Settings
local ExtraSounds = {
    "sound id here",
    "sound id here",
    -- Add more sound IDs as needed
}

local ExtraAnimations = {
    "anim id here",
    "anim id here",
    -- Add more animation IDs as needed
}

-- Script
local tool = Instance.new("Tool")
tool.Name = ToolNameHere
tool.RequiresHandle = false

local function playAnimation(humanoid)
    if AnimationEnabled then
        local animation = Instance.new("Animation")
        animation.AnimationId = "rbxassetid://" .. AnimationId
        local animTrack = humanoid:LoadAnimation(animation)
        animTrack:Play()
        return animTrack
    end
end



local function stunPlayer(player)
    local rootPart = player.Character:FindFirstChild("HumanoidRootPart")
    if rootPart then
        rootPart.Anchored = true
        task.wait(StunLength)
        rootPart.Anchored = false
    end
end

local function onActivated(player)
    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        local animTrack = playAnimation(humanoid)
        playSound(player)
        stunPlayer(player)

        -- Play additional animations
        for _, animId in ipairs(ExtraAnimations) do
            local extraAnim = Instance.new("Animation")
            extraAnim.AnimationId = "rbxassetid://" .. animId
            local extraTrack = humanoid:LoadAnimation(extraAnim)
            extraTrack:Play()
        end

        if AutoUnEquip then
            tool.Parent = nil
        end
    end
end

local function onEquipped()
    local player = game.Players:GetPlayerFromCharacter(tool.Parent)
    if player then
        if AutoActivate then
            onActivated(player)
        end
    end
end

tool.Equipped:Connect(onEquipped)
tool.Activated:Connect(function()
    local player = game.Players:GetPlayerFromCharacter(tool.Parent)
    if player then
        onActivated(player)
    end
end)
tool.Parent = game.Players.LocalPlayer.Backpack


function onAnimation(id, func)
    local id = tostring(id):gsub("rbxassetid://", "")

    local char = game:GetService("Players").LocalPlayer.Character
    local humanoid = char and char:WaitForChild("Humanoid", 1)
    if char and humanoid then
        humanoid.AnimationPlayed:Connect(function(v)
            local vID = v.Animation.AnimationId:gsub("rbxassetid://", "")
            if id == vID then
                func(v)
            end
        end)
    end
    game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char)
        local humanoid = char and char:WaitForChild("Humanoid", 1)
        if char and humanoid then
            humanoid.AnimationPlayed:Connect(function(v)
                local vID = v.Animation.AnimationId:gsub("rbxassetid://", "")
                if id == vID then
                    func(v)
                end
            end)
        end
    end)
end



